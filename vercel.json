{
    "version": 2,
    "builds": [
     {
      "src": "app.js",
      "use": "@vercel/node",
      "config": { "includeFiles":
       ["public/**",
        "public/css/**",
        "public/js/**",
        "views/**"] }
     }
    ],
    "routes": [
     {
      "src": "/(.*)",
      "dest": "app.js"
     }
    ]
   }
